# Environment
ENV=development  # Set to 'production' for production deployment

# Google Cloud Vertex AI Configuration
PROJECT_ID=your-google-cloud-project-id
LOCATION=us-central1  # Or your preferred GCP region

# FastAPI Configuration
LOG_LEVEL=INFO

# Database Configuration (for production)
DB_USER=postgres
DB_PASSWORD=your-strong-password
DB_NAME=kilokokua
DB_HOST=your-cloud-sql-instance-connection-name

# CORS Configuration
CORS_ORIGINS=https://kilokokua-frontend-[hash].run.app,http://localhost:3000

# JWT Authentication
SECRET_KEY=your-secret-key-for-jwt-tokens
